{
  "expo": {
    "name": "Mythos Cup",
    "slug": "mythos-cup-native",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.marcpuigdemont.mythoscupnative"
    },
    "android": {
      "package": "com.marcpuigdemont.mythoscupnative"
    },
    "description": "Arkham Horror third edition Mythos Cup project on React Native",
    "githubUrl": "https://github.com/MarcPuigdemont/mythos-cup-native"
  }
}